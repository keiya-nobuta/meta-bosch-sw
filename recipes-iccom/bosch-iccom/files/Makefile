CONFIG_BOSCH_ICCOM := m
CONFIG_BOSCH_ICCOM_SOCKETS := m
CONFIG_BOSCH_ICCOM_EXAMPLE := m
CONFIG_BOSCH_SYMSPI_SPI_MASTER := m
CONFIG_BOSCH_SYMSPI := m

export CONFIG_BOSCH_ICCOM
export CONFIG_BOSCH_ICCOM_SOCKETS
export CONFIG_BOSCH_ICCOM_EXAMPLE
export CONFIG_BOSCH_SYMSPI_SPI_MASTER
export CONFIG_BOSCH_SYMSPI

SRC := $(shell pwd)

all:
	$(MAKE) -C $(KERNEL_SRC) M=$(SRC)

modules_install:
	$(MAKE) -C $(KERNEL_SRC) M=$(SRC) modules_install

clean:
	$(MAKE) -C $(KERNEL_SRC) M=$(SRC) clean
